class Node
    attr_accessor :val
    attr_accessor :left
    attr_accessor :right

    def initialize(val)
        @val = val
    end
end

class DepthFirstSearch

    def getNode(node, val)
        stack = []
        stack.push(node)
        until(stack.empty?)
            next_node = stack.pop
            p next_node.val
            if(next_node.val == val)
                return next_node
            end
            if(next_node.right != nil)
                stack.push(next_node.right)
            end
            if(next_node.left != nil)
                stack.push(next_node.left)
            end
        end
    end
end

